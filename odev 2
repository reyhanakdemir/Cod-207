void setup() {

//set the window size
size(400,400);
//set the framerate
frameRate(60);
  
//print some text to console
println("I'm starting");
}

void draw(){
//set bacground refresh color
background(#F39C12);

fill(#ECF0F1);
stroke(#E67E22);

for(int y = 0 ; y <= height+60; y = y += 60) {
  for(int x = 0 ; x <= width+60; x = x += 60) {
   strokeWeight(9);
   stroke(#E67E22);
   fill(#ECF0F1);
   rect(x,y,35,35);
   ellipse(x,y,15,15);
  }
}
}
