void setup() {

//set the window size
size(400,400);
//set the framerate
frameRate(60);
  
//print some text to console
println("I'm starting");
}

void draw(){
//set bacground refresh color
background(0);

fill(60,50,90);

for(int y = 0 ; y <= height+45; y = y += 45) {
  for(int x = 0 ; x <= width+45; x = x += 45) {
   fill(90,20,90);
   ellipse(x,y,45,45);
  }
}
}
